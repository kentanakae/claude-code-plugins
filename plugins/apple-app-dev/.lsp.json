{
  "swift": {
    "command": "sourcekit-lsp",
    "extensionToLanguage": {
      ".swift": "swift"
    }
  },
  "clangd": {
    "command": "clangd",
    "extensionToLanguage": {
      ".c": "c",
      ".h": "c",
      ".cpp": "cpp",
      ".cxx": "cpp",
      ".cc": "cpp",
      ".hpp": "cpp",
      ".m": "objective-c",
      ".mm": "objective-cpp"
    }
  }
}
